{
    "name": "Jonda",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Qiitaでストックした記事が既読かどうか記録する拡張",
    "permissions": ["storage"],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "content_scripts": [
        {
            "matches": ["http://qiita.com/*"],
            "js": ["jquery.min.js","script.js"],
            "run_at": "document_end"
        }
    ]
}
